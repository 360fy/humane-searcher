{
  "name": "humane-searcher",
  "version": "1.8.3",
  "description": "Searcher module for Humane Discovery",
  "repository": "git@github.com:360fy/humane-searcher.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.4.1",
    "events": "1.1.1",
    "humane-node-commons": "1.3.1",
    "joi": "9.0.1",
    "lodash": "4.13.1",
    "md5": "2.1.0",
    "performance-now": "0.2.0"
  },
  "devDependencies": {
    "babel-core": "6.10.4",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-node5": "11.1.0",
    "eslint": "3.1.1",
    "eslint-config-360fy": "1.1.10",
    "eslint-plugin-react": "5.2.2"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5"
    ]
  }
}
