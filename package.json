{
  "name": "humane-searcher",
  "version": "2.0.0",
  "description": "Searcher module for Humane Discovery",
  "repository": "git@github.com:360fy/humane-searcher.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.4.6",
    "events": "1.1.1",
    "humane-node-commons": "1.4.2",
    "joi": "9.2.0",
    "lodash": "4.17.2",
    "md5": "2.2.1",
    "performance-now": "0.2.0",
    "qs": "6.3.0"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-node5": "11.1.0",
    "eslint": "3.10.2",
    "eslint-config-360fy": "1.1.12",
    "eslint-plugin-react": "6.7.1"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5"
    ]
  }
}
