{
  "name": "humane-searcher",
  "version": "1.2.0",
  "description": "Searcher module for Humane Discovery",
  "repository": "git@github.com:360fy/humane-searcher.git",
  "scripts": {
    "lint": "eslint ./src/",
    "clean": "rm -rf lib/",
    "compile": "npm run lint && npm run clean && babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "author": "Shailendra Sharma",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.3.5",
    "events": "^1.1.0",
    "humane-node-commons": "^1.1.6",
    "joi": "^8.0.5",
    "lodash": "^4.11.2",
    "md5": "^2.1.0",
    "performance-now": "^0.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-node5": "^11.0.2",
    "eslint": "^2.8.0",
    "eslint-config-360fy": "^1.1.4",
    "eslint-plugin-react": "^4.3.0"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"
    ],
    "presets": [
      "node5"
    ]
  }
}
